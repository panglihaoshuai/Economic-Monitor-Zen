# ===========================================
# Supabase 配置（必需）
# 获取地址：https://supabase.com/dashboard > 项目 > Settings > API
# ===========================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# ===========================================
# NextAuth.js 配置（必需）
# 获取地址：https://console.cloud.google.com > APIs & Services > Credentials
# ===========================================
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=generate_a_random_string_at_least_32_characters
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret

# ===========================================
# FRED API 配置（必需）
# 获取地址：https://fred.stlouisfed.org/docs/api/api_key.html
# ===========================================
FRED_API_KEY=your_fred_api_key_32_characters

# ===========================================
# DeepSeek API（用户配置，加密存储）
# 获取地址：https://platform.deepseek.com
# ===========================================
DEEPSEEK_API_KEY=sk-your_deepseek_api_key

# ===========================================
# Resend 邮件服务（必需）
# 获取地址：https://resend.com
# ===========================================
RESEND_API_KEY=re_your_resend_api_key

# ===========================================
# 加密密钥（32字符，用于加密存储用户API key）
# ===========================================
ENCRYPTION_KEY=your_32_character_encryption_key

# ===========================================
# 应用配置
# ===========================================
NEXT_PUBLIC_APP_NAME=Economic Monitor
NEXT_PUBLIC_APP_URL=http://localhost:3000
